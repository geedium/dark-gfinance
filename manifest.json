{
  "description": "A dark interface for Google Finance.",
  "manifest_version": 2,
  "name": "Dark Mode for Google Finance on Firefox",
  "version": "1.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/apply-css",
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/finance*"
      ],
      "css": [
        "dark-mode.css"
      ],
      "js": [
        "replace-svg.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "img/landingpage4.svg"
  ]
}